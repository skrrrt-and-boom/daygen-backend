{
  "name": "daygen-backend",
  "version": "1.0.0",
  "description": "Backend API for DayGen image generation platform",
  "author": "DayGen Team",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/src/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "lint:check": "eslint \"{src,apps,libs}/**/*.ts\"",
    "typecheck": "tsc -p tsconfig.build.json --noEmit",
    "docs:toc": "node scripts/update-docs-toc.js",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:google-auth": "node ../test-google-auth.js",
    "debug:queue": "node scripts/debug-queue-system.js",
    "backup:db": "node scripts/backup-database.js",
    "backup:users": "node scripts/export-users.js",
    "backup:full": "npm run backup:db && npm run backup:users",
    "backup:manual": "npm run backup:full && git add backups/ && git commit -m 'Database backup $(date)' && git push",
    "backup:check-connection": "node scripts/get-supavisor-connection.js",
    "sync:stripe": "node scripts/sync-stripe-payments.js",
    "sync:stripe:dry-run": "node scripts/sync-stripe-payments.js --dry-run",
    "test:r2": "NODE_OPTIONS='--openssl-legacy-provider' node scripts/test-r2.js",
    "migrate:images": "node scripts/migrate-images-to-r2.js",
    "prisma:generate": "prisma generate",
    "prisma:push": "prisma db push",
    "prisma:migrate": "prisma migrate dev",
    "prisma:studio": "prisma studio",
    "smoke:server": "node scripts/smoke-server.js",
    "smoke:db": "node scripts/smoke-db.js",
    "smoke:payments": "node scripts/smoke-payments.js",
    "smoke:queue": "node scripts/smoke-queue.js",
    "smoke:generate": "node scripts/smoke-generate.js",
    "smoke:frontend": "node scripts/smoke-frontend-integration.js",
    "smoke:auth": "node scripts/smoke-google-auth.js"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.913.0",
    "@aws-sdk/node-http-handler": "^3.370.0",
    "@aws-sdk/s3-request-presigner": "^3.896.0",
    "@elevenlabs/elevenlabs-js": "^2.25.1",
    "@google-cloud/tasks": "^6.2.0",
    "@nestjs/common": "^11.1.6",
    "@nestjs/config": "^4.0.2",
    "@nestjs/core": "^11.1.6",
    "@nestjs/jwt": "^11.0.0",
    "@nestjs/mapped-types": "^2.1.0",
    "@nestjs/passport": "^11.0.5",
    "@nestjs/platform-express": "^11.1.6",
    "@nestjs/platform-socket.io": "^11.1.6",
    "@nestjs/terminus": "^11.0.0",
    "@nestjs/websockets": "^11.1.6",
    "@prisma/client": "^6.17.0",
    "@supabase/supabase-js": "^2.75.0",
    "axios": "^1.12.2",
    "bcrypt": "^6.0.0",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.1",
    "cors": "^2.8.5",
    "dotenv": "^17.2.3",
    "google-auth-library": "^10.4.0",
    "jsonwebtoken": "^9.0.2",
    "lumaai": "^1.17.3",
    "multer": "^2.0.2",
    "nestjs-pino": "^4.4.1",
    "node-fetch": "^2.7.0",
    "passport": "^0.7.0",
    "passport-jwt": "^4.0.1",
    "passport-strategy": "^1.0.0",
    "pino": "^9.13.1",
    "pino-http": "^10.5.0",
    "pino-pretty": "^13.1.2",
    "prom-client": "^15.1.3",
    "reflect-metadata": "^0.2.2",
    "rxjs": "^7.8.2",
    "socket.io": "^4.8.1",
    "stripe": "^19.1.0",
    "superagent": "^10.2.3",
    "undici": "^7.16.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.38.0",
    "@jest/test-sequencer": "^30.2.0",
    "@nestjs/cli": "^11.0.10",
    "@nestjs/schematics": "^11.0.7",
    "@nestjs/testing": "^11.1.6",
    "@types/bcrypt": "^5.0.2",
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.12",
    "@types/jsonwebtoken": "^9.0.6",
    "@types/multer": "^1.4.11",
    "@types/node": "^20.14.9",
    "@types/passport-jwt": "^4.0.1",
    "@types/supertest": "^6.0.2",
    "@typescript-eslint/eslint-plugin": "^8.44.0",
    "@typescript-eslint/parser": "^8.44.0",
    "eslint": "^9.36.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.2.1",
    "globals": "^16.4.0",
    "jest": "^30.1.3",
    "prettier": "^3.6.2",
    "prisma": "^6.17.0",
    "source-map-support": "^0.5.21",
    "supertest": "^7.1.4",
    "ts-jest": "^29.4.4",
    "ts-loader": "^9.5.4",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.6.3",
    "typescript-eslint": "^8.46.0"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}